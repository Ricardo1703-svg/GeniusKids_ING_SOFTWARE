        android:text="Contenido 1"
        android:textColor="#E8D7D7"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeWidth="3dp" />

    <Button
        android:id="@+id/btnEva3Mate"
        android:layout_width="179dp"
        android:layout_height="65dp"
        android:layout_marginTop="76dp"
        android:backgroundTint="#F38D8D"
        android:text="Evaluacion 3"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEva2Mate"
        app:strokeWidth="3dp" />

    <Button
        android:id="@+id/btnEva1Mate"
        android:layout_width="179dp"
        android:layout_height="65dp"
        android:layout_marginTop="380dp"
        android:backgroundTint="#F38D8D"
        android:text="Evaluacion 1"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeWidth="3dp" />

    <Button
        android:id="@+id/btnEva2Mate"
        android:layout_width="179dp"
        android:layout_height="65dp"
        android:layout_marginTop="104dp"
        android:backgroundTint="#F38D8D"
        android:text="Evaluacion 2"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
